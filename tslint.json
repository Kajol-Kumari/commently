{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-xo/space"
  ],
  "jsRules": {},
  "rules": {
    "comment-format": false,
    "object-curly-spacing": false,
    "quotemark": [true, "single", "avoid-escape"],
    "object-literal-sort-keys": false,
    "typedef": false
  },
  "rulesDirectory": []
}
